{
    "name": "jskrd/php-prpdf",
    "description": "This library provides a set of classes for cleanly building print ready PDF files.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-imagick": "*",
        "dompdf/dompdf": "^0.8.3",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Jskrd\\PrintReadyPDF\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
